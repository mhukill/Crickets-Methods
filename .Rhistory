position.label <- function(xvals, yvals, pos.vec){ # finds coordinates for letter label in figure
# IN: various xvals
xlims <- range(xvals)
xcoord <- xlims[1] + (xlims[2] - xlims[1])*pos.vec[1]
ylims <- range(yvals)
ycoord <- ylims[1] + (ylims[2] - ylims[1])*pos.vec[2]
return(c(xcoord, ycoord))
}
